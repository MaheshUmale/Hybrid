# Application Run Mode: "live" or "simulation"
run.mode=simulation

# Live Data Source: "upstox" or "tradingview" (requires tv_data_bridge.py running)
live.data.source=upstox

# Upstox API Access Token (only required for "live" mode if source is upstox)
upstox.accessToken=YOUR_ACCESS_TOKEN_HERE

# QuestDB Integration
questdb.enabled=false

# Dashboard UI
dashboard.enabled=true

# Simulation event delay in milliseconds
simulation.event.delay.ms=10

# Replay source: "sample_data" (for now)
replay.source=sample_data

live.data.source=tradingview
run.mode=live